      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RegistrationForm"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AskAuthorisation">

    <Grid Background="#FF79B88F">
        <Button x:Name="Autho" Content="Войти" Margin="290,122,290,282" MaxHeight="46" MaxWidth="220" Click="Autho_Click" Background="#FFE9D09A" FontFamily="Comic Sans MS" BorderBrush="#FFE9D09A" FontSize="14" Foreground="#FF694D10"/>
        <Button x:Name="Register" Content="Зарегистрироваться" Margin="290,225,290,179" MaxHeight="46" MaxWidth="220" Click="Register_Click" Background="#FFE9D09A" FontFamily="Comic Sans MS" FontSize="14" BorderBrush="#FFE9D09A" Foreground="#FF694D10"/>

    </Grid>
</Page>
